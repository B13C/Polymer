{
  "name": "artful/polymer",
  "description": "Polymer是一个灵活并且可自由组合的PHP框架,基于slim+doctrine+twig",
  "license": "proprietary",
  "keywords": [
    "Polymer",
    "Slim-MVC",
    "Doctrine",
    "Twig"
  ],
  "prefer-stable": true,
  "require": {
    "php": ">=7.4",
    "slim/psr7": "1.4",
    "slim/slim": ">=4",
    "slim/flash": ">=0.4",
    "slim/csrf": ">=1.2",
    "slim/twig-view": ">=3.2",
    "tuupola/cors-middleware": ">=1.2",
    "tuupola/slim-jwt-auth": ">=2.3",
    "doctrine/inflector": ">=1.1",
    "doctrine/orm": ">=2.9",
    "beberlei/doctrineextensions": ">=1.3",
    "gedmo/doctrine-extensions": ">=3.1",
    "doctrine/migrations": ">=3",
    "monolog/monolog": ">=2",
    "hassankhan/config": ">=2",
    "peppeocchi/php-cron-scheduler": ">=4",
    "symfony/validator": ">=5.3",
    "symfony/serializer": ">=5.3",
    "symfony/property-access": ">=5.3",
    "symfony/property-info": ">=5.3",
    "symfony/expression-language": ">=5.3",
    "php-amqplib/php-amqplib": ">=3.0",
    "php-di/php-di": ">=6.3",
    "php-di/slim-bridge": ">=3.1",
    "ext-pdo": "*",
    "ext-simplexml": "*",
    "ext-exif": "*",
    "goaop/framework": ">=3.0",
    "dto/dto": ">=3.2"
  },
  "require-dev": {
    "roave/security-advisories": "dev-master",
    "guzzlehttp/guzzle": ">=6.2",
    "vlucas/phpdotenv": ">=2.4",
    "mikey179/vfsStream": ">=1.6",
    "mockery/mockery": ">=0.9",
    "satooshi/php-coveralls": ">=1.0",
    "phpunit/phpunit": ">=8",
    "facebook/webdriver": ">=1.0",
    "mysql-workbench-schema-exporter/mysql-workbench-schema-exporter": ">=3.1",
    "mysql-workbench-schema-exporter/doctrine2-exporter": ">=3.1"
  },
  "autoload": {
    "psr-4": {
      "Polymer\\": "src\\",
      "Polymer\\Tests\\": "tests\\"
    },
    "files": [
      "src\\Lib\\functions_include.php"
    ]
  },
  "repositories": {
    "packagist": {
      "type": "composer",
      "url": "https://packagist.phpcomposer.com"
    }
  }
}
